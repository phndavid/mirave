<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.master.ndavid.calirapido" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ActivityPlay"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityQuestion"
            android:label="@string/title_activity_question" >
        </activity>
        <activity
            android:name=".ActivityAnswer"
            android:label="@string/title_activity_answer" >
        </activity>
        <activity
            android:name=".ActivityBadAnswer"
            android:label="@string/title_activity_activity_bad_answer" >
        </activity>
        <activity
            android:name=".ActivityFeedBack"
            android:label="@string/title_activity_activity_feed_back" >
        </activity>
        <activity
            android:name=".ActivityGoodAnswer"
            android:label="@string/title_activity_activity_good_answer" >
        </activity>
    </application>

</manifest>
