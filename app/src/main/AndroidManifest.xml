<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.master.ndavid.calirapido" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/inicio_xhdpi"
        android:label="Calidoso"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ActivityPlay"
            android:label="@string/app_name"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityQuestion"
            android:label="@string/title_activity_question"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".ActivityBadAnswer"
            android:label="@string/title_activity_activity_bad_answer"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ActivityFeedBack"
            android:label="@string/title_activity_activity_feed_back"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ActivityGoodAnswer"
            android:label="@string/title_activity_activity_good_answer"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".ActivityIntro"
            android:label="@string/title_activity_activity_intro"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
